
<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Admin Panel | Öncü Hukuk</title>
    <link rel="apple-touch-icon" sizes="180x180" href="~/Content/Site/images/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Content/Site/images/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/~/Content/Site/images/favicon/favicon-16x16.png">
    <link rel="manifest" href="~/Content/Site/images/favicon/site.webmanifest">
    <link rel="mask-icon" href="~/Content/Site/images/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/Content/Admin/plugins/bootstrap/css/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Admin/css/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Admin/css/themes/all-themes.css")" rel="stylesheet" />
</head>

<body class="theme-red">

    <!-- #Top Bar -->
    @Html.Partial("_TopBar")

    <section>
        <!-- Left Sidebar -->
        @Html.Partial("_LeftsideBar")
        <!-- #END# Left Sidebar -->
        <!-- Right Sidebar -->
        @Html.Partial("_RightsideBar")
        <!-- #END# Right Sidebar -->
    </section>

    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>

    <!-- Jquery Core Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/jquery/jquery.min.js")"></script>

    <!-- Bootstrap Core Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/bootstrap/js/bootstrap.js")"></script>

    <!-- Select Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/bootstrap-select/js/bootstrap-select.js")"></script>

    <!-- Slimscroll Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/jquery-slimscroll/jquery.slimscroll.js")"></script>

    <!-- Waves Effect Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/node-waves/waves.js")"></script>

    <!-- Jquery CountTo Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/jquery-countto/jquery.countTo.js")"></script>

    <!-- Morris Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/raphael/raphael.min.js")"></script>
    <script src="@Url.Content("~/Content/Admin/plugins/morrisjs/morris.js")"></script>

    <!-- ChartJs -->
    <script src="@Url.Content("~/Content/Admin/plugins/chartjs/Chart.bundle.js")"></script>

    <!-- Flot Charts Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/flot-charts/jquery.flot.js")"></script>
    <script src="@Url.Content("~/Content/Admin/plugins/flot-charts/jquery.flot.resize.js")"></script>
    <script src="@Url.Content("~/Content/Admin/plugins/flot-charts/jquery.flot.pie.js")"></script>
    <script src="@Url.Content("~/Content/Admin/plugins/flot-charts/jquery.flot.categories.js")"></script>
    <script src="@Url.Content("~/Content/Admin/plugins/flot-charts/jquery.flot.time.js")"></script>

    <!-- Sparkline Chart Plugin Js -->
    <script src="@Url.Content("~/Content/Admin/plugins/jquery-sparkline/jquery.sparkline.js")"></script>

    <!-- Custom Js -->
    <script src="@Url.Content("~/Content/Admin/js/admin.js")"></script>
    <script src="@Url.Content("~/Content/Admin/js/pages/index.js")"></script>

    <!-- Demo Js -->
    <script src="@Url.Content("~/Content/Admin/js/demo.js")"></script>
</body>

</html>
